<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!--<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />-->


    <!--##########################################################################################-->
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>


    <!-- Icon -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


    <!-- JQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>



    <!-- Bootstrap table CSS -->
    <link href="https://unpkg.com/bootstrap-table@1.21.0/dist/bootstrap-table.min.css" rel="stylesheet">
    <!-- Bootstrap table JS -->
    <script src="https://unpkg.com/bootstrap-table@1.21.0/dist/bootstrap-table.min.js"></script>


    <!--Bootstrap Table Resizable -->
    <script src="https://unpkg.com/bootstrap-table@1.21.0/dist/extensions/resizable/bootstrap-table-resizable.min.js"></script>


    <!-- Bootstrap Table Copy -->
    <script src="https://unpkg.com/bootstrap-table@1.21.0/dist/extensions/copy-rows/bootstrap-table-copy-rows.min.js"></script>


    <!-- Bootstrap Table Export -->
    <script src="https://unpkg.com/bootstrap-table@1.21.0/dist/extensions/export/bootstrap-table-export.min.js"></script>


    <!-- Bootstrap Table Print -->
    <script src="https://unpkg.com/bootstrap-table@1.21.0/dist/extensions/print/bootstrap-table-print.min.js"></script>

    <!-- JQuery resizableColumns CSS -->
    <link href="https://unpkg.com/jquery-resizable-columns@0.2.3/dist/jquery.resizableColumns.css" rel="stylesheet">
    <!-- JQuery resizableColumns JS -->
    <script src="https://unpkg.com/jquery-resizable-columns@0.2.3/dist/jquery.resizableColumns.min.js"></script>



    <!-- JQuery TableExport -->
    <script src="https://cdn.jsdelivr.net/npm/tableexport.jquery.plugin@1.10.21/tableExport.min.js"></script>
    <!-- JQuery TableExport PDF -->
    <script src="https://cdn.jsdelivr.net/npm/tableexport.jquery.plugin@1.10.21/libs/jsPDF/jspdf.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/tableexport.jquery.plugin@1.10.21/libs/jsPDF-AutoTable/jspdf.plugin.autotable.js"></script>


    <!--##########################################################################################-->
</head>
<body>

    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">MyProfile</a>
                <div class="btn-group">
                    <button type="button" class="btn btn-outline-danger dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
                        <i class="bi bi-app-indicator"></i> Actions
                    </button>
                    <ul class="dropdown-menu">
                        <li><a class="nav-link text-dark bg-success rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                        <li><a class="nav-link text-dark bg-danger rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Resumes" asp-action="Index">Resumes</a></li>
                        <li> <a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="BasicInfoes" asp-action="Index">Basic-Info</a></li>
                        <li><a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Skills" asp-action="Index">Skills</a></li>
                        <li><a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Educations" asp-action="Index">Educations</a></li>
                        <li><a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Experiences" asp-action="Index">Experiences</a></li>
                        <li> <a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Projects" asp-action="Index">Projects</a></li>
                        <li><a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Certifications" asp-action="Index">Certifications</a></li>
                        <li><a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Languages" asp-action="Index">Languages</a></li>
                        <li> <a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="ContactUs" asp-action="Index">Contact Us</a></li>
                        <li> <a class="nav-link text-dark bg-info rounded-5 mx-1 my-1 text-center" asp-area="" asp-controller="Other" asp-action="Index">Other</a></li>
                    </ul>
                </div>

            </div>
        </nav>
    </header>


    <div class="container">
        @RenderBody()
    </div>

    @RenderSection("Scripts", required: false)
</body>
</html>
